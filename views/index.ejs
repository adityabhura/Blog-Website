<%- include("partials/header") %>
<div class="blogs">
    <% blogs.forEach(function(blog){ %>
    <div>
        <h1><%= blog.title %></h1>
        <img src="<%= blog.image[0] %>">
        <span><%= blog.Created %></span>
        
        <a href="/blogs/<%= blog._id %>"><button>Read more</button></a>
    </div>
        <% }); %>
</div>
<%- include("partials/footer") %>
